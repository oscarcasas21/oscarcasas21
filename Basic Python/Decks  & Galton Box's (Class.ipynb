    "# YOUR CODE HERE\n",
    "class MarblesBoard:\n",
    "    \n",
    "    \n",
    "    def __init__(self, prog):\n",
    "        self.prog = list(prog)\n",
    "        \n",
    "    def switch(self):\n",
    "        y = self.prog[1]\n",
    "        self.prog[1] = self.prog[0]\n",
    "        self.prog[0] = y\n",
    "        \n",
    "    def rotate(self):\n",
    "        leng = len(self.prog)\n",
    "        x = self.prog[0]\n",
    "        for i in range(1, leng):   \n",
    "            self.prog[i-1] = self.prog[i]   \n",
    "        self.prog[leng-1] = x\n",
    "        \n",
    "    def is_solved(self):\n",
    "        leng = len(self.prog)\n",
    "        for i in range(leng): \n",
    "            if i == self.prog[i]:\n",
    "                return True  \n",
    "        return False\n",
    "    \n",
    "    def __str__(self):\n",
    "        return ' '.join(list(map(str,self.prog)))\n",
    "    \n",
    "\n",
    "class Solver:\n",
    "\n",
    "    def __init__(self, game):\n",
    "        self.game = game\n",
    "        \n",
    "    def solve(self):\n",
    "        step = 0\n",
    "        while self.game.is_solved() == False:\n",
    "            if  self.game.prog[1] < self.game.prog[0] and self.game.prog[0] != len(self.game.prog) - 1:\n",
    "                self.game.switch()\n",
    "            else:\n",
    "                self.game.rotate()\n",
    "            print(self.board)\n",
    "            step = step + 1\n",
    "        print('total steps:', step)\n",
    "\n",
    "raise NotImplementedError()"
   ]
